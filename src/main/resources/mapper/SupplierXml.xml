<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dapang.recharge.mapper.SupplierMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.dapang.recharge.pojo.po.SupplierPO">
        <result column="name" property="name" />
        <result column="rate" property="rate" />
        <result column="contact_info" property="contactInfo" />
        <result column="address" property="address" />
        <result column="status" property="status" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        name, rate, contact_info, address, status
    </sql>

</mapper>
